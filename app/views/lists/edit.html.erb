<header>
</header>

<div class="list">
  <p class="new-list">リスト名を変更</p>
  <%= form_with(model: @list, local: true) do |form| %>
    <div class="name">
      リスト名を入力してください
      <%= form.text_field :name, class:"form-tag-box"%>
    </div>
    <div class="submit">
      <%= form.submit class:"submit-box"%>
    </div>
  <% end %>
</div>
